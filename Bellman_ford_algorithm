#include<iostream>
#include<bits/stdc++.h>
using namespace std;
struct node{
	int u;
	int v;
	int wt;
	node(int a,int b,int weight)
	{
		u=a;
		v=b;
		wt=weight;
	}
};

int main()
{
	int n,m;
	cout<<"Enter the total number of vertices and edges of the graph"<<"\n";
	cin>>n>>m;
	vector<node> edges;
	int u,v,wt,i,j;
	for(i=0;i<m;i++)
	{
		cin>>u>>v>>wt;
		edges.push_back(node(u,v,wt));
	}
	int src;
	cin>>src;
	int max_dis=10000;
	int dist[n];
	
	for(i=0;i<n;i++)
	dist[i]=max_dis;
	
	dist[src]=0;
	
	for(i=1;i<=(n-1);i++)
	{
		for(j=0;j<edges.size();j++)
		{
			if(dist[edges[j].u]+edges[j].wt<dist[edges[j].v])
			{
				dist[edges[j].v]=dist[edges[j].u]+edges[j].wt;
			}
		}
	}
	    int flag=1;
	    for(j=0;j<edges.size();j++)
		{
			if(dist[edges[j].u]+edges[j].wt<dist[edges[j].v])
			{
				cout<<"Negative cycle"<<"\n";
				flag=0;
				break;
			}
		}
		if(flag==1)
		{
		    for(i=0;i<n;i++)
			{
				cout<<dist[i]<<" ";
			}	
		}
}
